server {
  listen 80;
  server_name localhost;

  #charset koi8-r;

  #access_log logs/host.access.log main;

  location / {
    root /home/common;
    index index.html index.htm
  }

  error_page 404 /404.html

  err_page 500 52 503 504 /50x.html

  location = /50x.html {
    root html;
  }

}

server {
  listen 80;
  server_name www.myarki.com;
  location /{
    proxy_pass http://127.0.0.1:9090
  }
}
